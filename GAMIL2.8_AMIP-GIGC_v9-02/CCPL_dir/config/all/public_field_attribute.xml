<?xml version="1.0" ?>
<field name="evap"  dimensions="0D"   long_name="water evaporation"  type="flux"  default_unit="kg/s/m^2" />
<field name="t"  dimensions="V3D"   long_name="surface temperature"  type="state"  default_unit="kelvin" />
<field name="ps"  dimensions="H2D"   long_name="surface pressure"  type="state"  default_unit="Pa" />
<field name="ifrac"  dimensions="H2D"   long_name="fraction of sea ice"  type="state"  default_unit="" />
<field name="sst"  dimensions="V3D"   long_name="sea surface temperature"  type="state"  default_unit="kelvin" />
<field name="sss"  dimensions="V3D"   long_name="salty surface temperature"  type="state"  default_unit="kelvin" />
<field name="tbot"  dimensions="V3D"   long_name="bottom atm level temperature"  type="state"  default_unit="kelvin" />
<field name="u_atm_bot"  dimensions="V3D"   long_name="bottom atm level zonal wind velocity"  type="state"  default_unit="m/s^2" />
